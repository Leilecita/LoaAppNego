<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/card_view_mistake"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/rec_dialog"
    android:backgroundTint="@color/background"

    >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/dialog_margin_bottom"
        android:layout_marginTop="@dimen/dialog_margin_top"
        android:layout_marginLeft="@dimen/dialog_margin_left"
        android:layout_marginRight="@dimen/dialog_margin_right"
        android:orientation="vertical">


        <TextView
            android:layout_width="match_parent"
            android:layout_height="36dp"
            android:text="Informacion caja"
            android:textSize="18sp"
            android:textColor="@color/word"
            android:gravity="center"
            android:textStyle="bold"
            />


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="6dp"
            android:orientation="horizontal">
            <TextView
                android:layout_height="wrap_content"
                android:layout_width="130dp"
                android:padding="4dp"
                android:textSize="@dimen/general_text"
                android:fontFamily="@font/opensansregular"
                android:textColor="@color/colorPrimaryDarkLetter"
                android:text="Fecha  "/>

            <TextView
                android:id="@+id/date"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:padding="4dp"
                android:textColor="@color/colorDialogButton"
                android:textSize="18sp"
                android:textStyle="italic"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_marginTop="4dp"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <TextView
                android:layout_height="wrap_content"
                android:layout_width="130dp"
                android:padding="4dp"
                android:textSize="@dimen/general_text"
                android:fontFamily="@font/opensansregular"
                android:textColor="@color/colorPrimaryDarkLetter"
                android:text="Vta contado"/>

            <TextView
                android:id="@+id/counted_sale"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:padding="4dp"
                android:textColor="@color/colorDialogButton"
                android:textSize="18sp"
                android:textStyle="italic"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_marginTop="4dp"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <TextView
                android:layout_height="wrap_content"
                android:layout_width="130dp"
                android:padding="4dp"
                android:textSize="@dimen/general_text"
                android:fontFamily="@font/opensansregular"
                android:textColor="@color/colorPrimaryDarkLetter"
                android:text="Tarjeta "/>

            <TextView
                android:id="@+id/credit_card"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:padding="4dp"
                android:textColor="@color/colorDialogButton"
                android:textSize="18sp"
                android:textStyle="italic"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_marginTop="4dp"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <TextView
                android:layout_height="wrap_content"
                android:layout_width="130dp"
                android:padding="4dp"
                android:textSize="@dimen/general_text"
                android:fontFamily="@font/opensansregular"
                android:textColor="@color/colorPrimaryDarkLetter"
                android:text="Caja total "/>

            <TextView
                android:id="@+id/total_box"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:padding="4dp"
                android:textColor="@color/colorDialogButton"
                android:textSize="18sp"
                android:textStyle="italic"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_marginTop="4dp"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <TextView
                android:layout_height="wrap_content"
                android:layout_width="130dp"
                android:padding="4dp"
                android:textSize="@dimen/general_text"
                android:fontFamily="@font/opensansregular"
                android:textColor="@color/colorPrimaryDarkLetter"
                android:text="Resto caja "/>

            <TextView
                android:id="@+id/rest_box"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:padding="4dp"
                android:textColor="@color/colorDialogButton"
                android:textSize="18sp"
                android:textStyle="italic"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="4dp"
            android:orientation="horizontal">
            <TextView
                android:layout_height="wrap_content"
                android:layout_width="130dp"
                android:padding="4dp"
                android:textSize="@dimen/general_text"
                android:fontFamily="@font/opensansregular"
                android:textColor="@color/colorPrimaryDarkLetter"
                android:text="Depo/Extr "/>

            <TextView
                android:id="@+id/deposit"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:padding="4dp"
                android:textColor="@color/colorDialogButton"
                android:textSize="18sp"
                android:textStyle="italic"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="4dp"
            android:orientation="horizontal">
            <TextView
                android:layout_height="wrap_content"
                android:layout_width="130dp"
                android:padding="4dp"
                android:textSize="@dimen/general_text"
                android:fontFamily="@font/opensansregular"
                android:textColor="@color/colorPrimaryDarkLetter"
                android:text="Resto caja ant"/>

            <TextView
                android:id="@+id/rest_box_ant"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:padding="4dp"
                android:textColor="@color/colorDialogButton"
                android:textSize="18sp"
                android:textStyle="italic"/>
        </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="4dp"
            android:orientation="horizontal">
            <TextView
                android:layout_height="wrap_content"
                android:layout_width="130dp"
                android:padding="4dp"
                android:textSize="@dimen/general_text"
                android:fontFamily="@font/opensansregular"
                android:textColor="@color/colorPrimaryDarkLetter"
                android:text="Detalle "/>

            <TextView
                android:id="@+id/detail"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:padding="4dp"
                android:textColor="@color/colorDialogButton"
                android:textSize="18sp"
                android:textStyle="italic"/>
        </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="4dp"
            android:orientation="horizontal">

            <ImageView
                android:layout_width="match_parent"
                android:id="@+id/edith"
                android:layout_height="40dp"
                android:layout_weight="1"
                android:src="@drawable/edituser"
                android:padding="8dp"
                android:textColor="#6e6e6e"
                android:textSize="18dp"
                android:textStyle="italic"/>

            <ImageView
                android:layout_height="40dp"
                android:layout_width="match_parent"
                android:layout_weight="1"
                android:padding="8dp"
                android:id="@+id/delete"
                android:src="@drawable/deleteuser"
                android:textSize="18dp"
                android:textStyle="bold"
                android:textColor="#414141"
                />
        </LinearLayout>

    </LinearLayout>
    </LinearLayout>


